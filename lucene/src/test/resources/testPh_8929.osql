create class Test IF NOT EXISTS;

create property Test.a STRING;
create property Test.b STRING;

create index Test.a_b on Test(a,b) FULLTEXT ENGINE LUCENE;


insert into Test set a="fox", b="cat";
insert into Test set a="dog", b="whale";
insert into Test set a="snake", b="elephant";
insert into Test set a="lion", b="tiger";

